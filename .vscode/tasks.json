{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: g++-10 build active file",
			"command": "/usr/local/bin/g++-10",
			"args": [
				"-g",
				"./*.cpp",
				"-o",
				"${workspaceFolder}/out/bin"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}